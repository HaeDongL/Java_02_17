class Prob08 
{
	//Field

	//Method
	public int abs(int x, int y){
		int result = x - y;
		if(result < 0){
			result *= -1;
			return result;
		}else{
			return result;
		}
	}

	public static void main(String[] args) 
	{
		int num1 = 6;
		int num2 = 15;

		Prob08 prob = new Prob08();
		System.out.println("두 수의 차는 "+prob.abs(num1, num2));
	}
}
