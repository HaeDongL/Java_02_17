package t03_04;

import java.io.BufferedReader;
import java.io.FileReader;
import java.util.Vector;

public class Prob3 {

	
	public Vector getList() throws Exception{
		
		BufferedReader br = new BufferedReader(new FileReader("list.txt"));
		Vector vc = new Vector();
		Vector vc2 = null;
		String read = "";
		String[] strSplit;
		while(true) {
			read = br.readLine();
			if(read == null) {
				break;
			}
			strSplit = read.split("/");
			if("java".equals(strSplit[1]) || "JAVA".equals(strSplit[1])) {
				for(String st : strSplit) {
					vc.add(st);
				}
				if(Integer.parseInt(strSplit[2]) >= 80 ) {
					vc.add("수료");
				}else {
					vc.add("미수료");
				}
			}
			
			if("sql".equals(strSplit[1]) || "SQL".equals(strSplit[1])) {
				for(String st : strSplit) {
					vc.add(st);
				}
				if(Integer.parseInt(strSplit[2]) >= 90 ) {
					vc.add("수료");
				}else {
					vc.add("미수료");
				}
			}
			vc2 = vc;
			
		}
		System.out.println(vc);
		return vc2;
}
	
	
	public static void main(String[] args) throws Exception {
		
		Prob3 e = new Prob3();
		
		Vector scores = e.getList();
		for(int i=0; i<scores.size(); i++) {
			System.out.println(scores.get(i));
		}
		
	}

}
