//=============================================//
1.DDL::CEREATE : TABLE 생성
//==> TABLE 생성
//==> SQL은 대소문자를 구별않음
//==> 권장사항 : 예약어(Keyword) : 대문자
			: 식별자(identifier): 소문자
			
CREAD TABLE emp_TEST(
	id			NUMBER(3),
	name		VARCHAR2(15),
	salary		NUMBER(7,2),
	title		VARCHAR2(15)		DEFAULT		'사원',
	in_date		DATE				DEFAULT		SYSDATE,

);

//=============================================//

DESC emp_test;

DESCRIBE emp_test;

SELECT * FROM emp_test;