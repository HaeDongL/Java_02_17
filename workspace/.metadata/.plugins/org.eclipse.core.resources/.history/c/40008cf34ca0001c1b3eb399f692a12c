CREATE TABLE SALGRADE(
grade NUMBER(2) CONSTRAINT salgrade_pk PRIMARY KEY,
losal NUMBER(5) CONSTRAINT losal_nl NOT NULL,
hisal NUMBER(5)	CONSTRAINT hisal_nl NOT NULL
);

INSERT
INTO SALGRADE
VALUES(1,1000,5000);
